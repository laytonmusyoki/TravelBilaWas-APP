        tools:targetApi="31">
        <activity
            android:name=".ParcelActivity"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".EventActivity"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".InstitutionActivity"
            android:parentActivityName=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".SoloActivity"
